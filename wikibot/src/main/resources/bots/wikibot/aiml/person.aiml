<?xml version="1.0" encoding="UTF-8"?>
<aiml>
    <category>
        <pattern>WHO IS *</pattern>
        <template>
            <think><set name="topic">Person</set></think>
            <think><set name="property">rdfs:comment</set></think>
            <think><set name="value"><star/></set></think>
            Here's what I found about
        </template>
    </category>

    <category>
        <pattern>* BIRTHDATE</pattern>
        <template>
            <think><set name="topic">Person</set></think>
            <think><set name="property">dbo:birthDate</set></think>
            <think><set name="value"><star/></set></think>
            response
        </template>
    </category>
    <category>
        <pattern>WHEN WAS * BORN</pattern>
        <template>
            <srai><star/> BIRTHDATE</srai>
        </template>
    </category>
    <category>
        <pattern>WHEN * BORN</pattern>
        <template>
            <srai><star/> BIRTHDATE</srai>
        </template>
    </category>
    <category>
        <pattern>* BIRTHDAY</pattern>
        <template>
            <srai><star/> BIRTHDATE</srai>
        </template>
    </category>
